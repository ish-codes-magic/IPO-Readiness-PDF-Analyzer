{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "app/main.py": {
      "runtime": "python3.11"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app/main"
    }
  ],
  "build": {
    "env": {
      "PYTHONPATH": "."
    }
  },
  "installCommand": "pip install -r requirements.txt"
}